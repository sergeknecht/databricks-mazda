{
    "Version": "2008-10-17",
    "Statement": [
        {
            "Sid": "DatabricksBucketPolicy-nonprd",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::289450402608:root"
            },
            "Action": [
                "s3:GetBucketLocation",
                "s3:ListBucket",
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject",
                "s3:PutObjectAcl",
                "s3:GetObjectVersion"
            ],
            "Resource": [
                "arn:aws:s3:::mazda-csv-data-nonprd",
                "arn:aws:s3:::mazda-csv-data-nonprd/*"
            ]
        }
    ]
}
